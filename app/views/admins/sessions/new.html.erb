  <div class="top_image_box">
    <div class="top_image">
      <div class="top_logo_box">
        <%= link_to image_tag("logo_black.png", :class => "top_logo"), root_path %>
      </div>
        <div class="devise_image_text_left">
          <div class="devise_line1">Log in</div>
        </div>
        <div class="col-lg-8 col-lg-offset2"%>
          <div class="devise_box">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
              <div class="field">
                <%= f.label :email %><br/>
                <%= f.email_field :email, autofocus: true, autocomplete: "email", style: "color:black;" %>
              </div>

              <div class="field">
                <%= f.label :password %><br/>
                <%= f.password_field :password, autocomplete: "off", style: "color:black;" %>
              </div>

              <% if devise_mapping.rememberable? -%>
                <div class="field">
                  <%= f.check_box :remember_me %>
                  <%= f.label :remember_me %>
                </div>
              <% end -%>

              <div class="actions">
                <%= f.submit "Log in", class: "btn btn-default", style:"color:black" %>
              </div>
            <% end %>
            <%= render "devise/shared/links" %>
          </div>
        </div>
    </div>
  </div>